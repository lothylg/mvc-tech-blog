            <div class="col-lg-12 p-3 bg-info row">
                <div class="col-lg-3"></div>
                <h3 class="col-lg-3"> Your Dashboard </h3>
            </div>
            </header>
            {{#if posts.length}}
            <div id="postContainer" class="col-12 gy-4">
                {{#each posts as |post|}}
                    <div id="{{post.dataValues.id}}" class="col-10 border bg-secondary p-2 d-flex justify-content-between m-4" style="cursor: pointer;" onclick="window.location.href='post/{{post.dataValues.id}}'">
                        <h5 class="text-light">{{post.dataValues.content}}</h3>
                        <p>{{post.dataValues.date_posted}}</p>
                    </div>
                {{/each}}
            </div>
        {{else}}
            <div id="noPostCont" class="col-12 gy-6 text-center">
                <h5>You have not posted yet, click below to create a new blog post!</h5>
                <button id="dashCreate" class="btn btn-dark">+ New Post</button>
            </div>
        {{/if}}